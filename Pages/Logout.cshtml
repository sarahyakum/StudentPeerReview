@page
@model StudentPR.Pages.LogoutModel
@{
    ViewData["Title"] = "Log In";
}

<!-- Displays name of student currently logged in -->
<div style="color: green; margin-bottom: 10px;">
    You are currently logged in as @HttpContext.Session.GetString("StudentName")
</div>

<!-- Form for logging out -->
<form method="post">
    <button type="logout">Logout</button>
</form>
